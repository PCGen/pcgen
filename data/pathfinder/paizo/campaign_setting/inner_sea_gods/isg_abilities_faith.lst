CATEGORY=Internal|Deific Obedience Benefits.MOD																																																																																																																																																																																																																																																																																																																							ABILITY:Special Ability|AUTOMATIC|Exalted Boon ~ Nethys ~ Magical Essences|PREMULT:4,[PREDEITY:1,Nethys],[PREMULT:1,[PREVARGTEQ:DeificBoonLVL,12],[PREVARGTEQ:ExaltedBoonLVL,3]],[!PRECLASS:1,Evangelist=1],[!PRECLASS:1,Sentinel=1]	ABILITY:Special Ability|AUTOMATIC|Exalted Boon ~ Nethys ~ Staff Channel|PREMULT:4,[PREDEITY:1,Nethys],[PREMULT:1,[PREVARGTEQ:DeificBoonLVL,16],[PREVARGTEQ:ExaltedBoonLVL,6]],[!PRECLASS:1,Evangelist=1],[!PRECLASS:1,Sentinel=1]	ABILITY:Special Ability|AUTOMATIC|Exalted Boon ~ Nethys ~ Pure Magic Aura|PREMULT:4,[PREDEITY:1,Nethys],[PREMULT:1,[PREVARGTEQ:DeificBoonLVL,20],[PREVARGTEQ:ExaltedBoonLVL,9]],[!PRECLASS:1,Evangelist=1],[!PRECLASS:1,Sentinel=1]
